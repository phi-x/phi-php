#!/usr/bin/env php
<?php

if (php_sapi_name() !== 'cli') {
    exit;
}

if (file_exists(__DIR__.'/../../../autoload.php')) {
    include __DIR__.'/../../../autoload.php';
} else {
    require __DIR__.'/../vendor/autoload.php';
}

$phi = new Phi\Program;

$code = $phi->execute($argv ?? $_SERVER['argv'] ?? []);

exit($code);
